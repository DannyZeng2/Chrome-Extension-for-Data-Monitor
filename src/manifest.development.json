{
    "manifest_version": 2,
    "name": "chrome-ext",
    "description": "chrome extension",
    "version": "0.0.1",
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
        "matches": [
            "*://*/*"
        ],
        "js": [
            "js/content.js"
        ],
        "run_at": "document_end"
    }],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    }
}